<h1> Lista de albums </h1>

<section class="listArtists">
    {{#each albumList}}
    <article class="eachArtist">
        <img src="{{this.images.0.url}}" alt="imagen" width="200px" />
        <div class="textAndLink">
            <p> {{this.name}} </p>
            <p><a href="/tracks/{{this.id}}"> View tracks </a></p>
        </div>
    </article>
    {{/each}}
</section>
